{
  "name": "webapp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "file-loader": "^4.0.0",
    "prettier-stylelint": "^0.4.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --config build-utils/webpack.config.js --env.env=dev",
    "build-staging": "webpack --config build-utils/webpack.config.js --env.env=staging",
    "build-prod": "webpack --config build-utils/webpack.config.js --env.env=prod",
    "deploy-staging": "webpack --config build-utils/webpack.config.js --env.env=staging && aws s3 sync dist/ s3://staging.bifocl.com --acl public-read",
    "deploy-prod": "webpack --config build-utils/webpack.config.js --env.env=prod && aws s3 sync dist/ s3://app.bifocl.com --acl public-read"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.14",
    "auth0-js": "^9.13.2",
    "autoprefixer": "^9.6.0",
    "axios": "^0.20.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "jquery": "^3.5.0",
    "lodash": "^4.17.19",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-dropzone": "^11.0.3",
    "react-icons": "^3.7.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "4.3.1",
    "react-select": "^3.0.4",
    "react-sizes": "^2.0.0",
    "react-wavesurfer": "^0.8.6",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "sass-loader": "^7.1.0",
    "wavesurfer.js": "1.4.0",
    "webpack-merge": "^4.2.1"
  }
}
